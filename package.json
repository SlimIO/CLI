{
    "name": "@slimio/cli",
    "version": "0.4.0",
    "description": "SlimIO Command Line Interface",
    "main": "index.js",
    "bin": {
        "slimio": "./bin/index.js"
    },
    "scripts": {
        "test": "cross-env psp && jest --coverage",
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/CLI.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "files": [
        "index.d.ts",
        "bin/index.js",
        "commands",
        "src"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/CLI/issues"
    },
    "homepage": "https://github.com/SlimIO/CLI#readme",
    "dependencies": {
        "@slimio/addon-factory": "^0.5.0",
        "@slimio/async-cli-spinner": "^0.5.1",
        "@slimio/bundler": "^0.4.1",
        "@slimio/config": "^0.15.1",
        "@slimio/core": "^0.11.0",
        "@slimio/installer": "^0.3.1",
        "@slimio/is": "^1.5.1",
        "@slimio/json-diff": "^0.1.0",
        "@slimio/lazy": "^1.2.0",
        "@slimio/lock": "^0.3.0",
        "@slimio/manifest": "^0.7.1",
        "@slimio/pretty-json": "^1.3.0",
        "@slimio/pretty-stack": "^0.4.1",
        "@slimio/stdin": "^1.0.3",
        "@slimio/tcp-sdk": "^0.4.6",
        "@slimio/utils": "^0.9.0",
        "@slimio/validate-addon-name": "^1.2.0",
        "cacache": "^13.0.1",
        "dotenv": "^8.1.0",
        "fast-levenshtein": "^2.0.6",
        "httpie": "^1.1.2",
        "kleur": "^3.0.3",
        "lodash.clonedeep": "^4.5.0",
        "lodash.get": "^4.4.2",
        "make-promises-safe": "^5.1.0",
        "ms": "^2.1.2",
        "qoa": "^0.2.0",
        "sade": "^1.6.1",
        "yn": "^3.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.0.0-next.3",
        "@slimio/psp": "^0.8.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.5.1",
        "husky": "^3.0.8",
        "jest": "^24.9.0",
        "jsdoc": "^3.6.3",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js src/**.js commands/**.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/test/**/*.js"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/test/fixtures/"
        ]
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
