{
    "name": "@slimio/cli",
    "version": "0.1.0",
    "description": "SlimIO Command Line Interface",
    "main": "index.js",
    "preferGlobal": true,
    "bin": {
        "slimio": "./bin/index.js"
    },
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/CLI.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/CLI/issues"
    },
    "homepage": "https://github.com/SlimIO/CLI#readme",
    "dependencies": {
        "@slimio/addon-factory": "^0.4.0",
        "@slimio/arg-parser": "^0.3.1",
        "@slimio/async-cli-spinner": "^0.1.2",
        "@slimio/github": "^0.4.2",
        "@slimio/lazy": "^1.2.0",
        "@slimio/manifest": "^0.4.0",
        "@slimio/tcp-sdk": "^0.3.3",
        "@slimio/utils": "^0.9.0",
        "dotenv": "^8.0.0",
        "kleur": "^3.0.3",
        "make-promises-safe": "^5.0.0",
        "qoa": "^0.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.6.1",
        "@commitlint/config-conventional": "^7.6.0",
        "@slimio/eslint-config": "^2.0.4",
        "eslint": "^5.16.0",
        "husky": "^2.2.0",
        "jsdoc": "^3.6.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
