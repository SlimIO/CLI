{
    "name": "@slimio/cli",
    "version": "0.2.6",
    "description": "SlimIO Command Line Interface",
    "main": "index.js",
    "bin": {
        "slimio": "./bin/index.js"
    },
    "scripts": {
        "test": "cross-env psp && ava --verbose",
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/CLI.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "files": [
        "index.ts",
        "index.d.ts",
        "bin/index.js",
        "commands/*.js",
        "src/*.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/CLI/issues"
    },
    "homepage": "https://github.com/SlimIO/CLI#readme",
    "dependencies": {
        "@slimio/addon-factory": "^0.4.0",
        "@slimio/async-cli-spinner": "^0.3.1",
        "@slimio/bundler": "^0.3.0",
        "@slimio/core": "^0.8.0",
        "@slimio/github": "^0.5.0",
        "@slimio/is": "^1.5.1",
        "@slimio/lazy": "^1.2.0",
        "@slimio/lock": "^0.1.0",
        "@slimio/manifest": "^0.6.0",
        "@slimio/pretty-json": "^1.2.0",
        "@slimio/tcp-sdk": "^0.4.6",
        "@slimio/utils": "^0.9.0",
        "cacache": "^12.0.2",
        "dotenv": "^8.0.0",
        "emphasize": "^2.1.0",
        "fast-levenshtein": "^2.0.6",
        "json-diff": "^0.5.4",
        "kleur": "^3.0.3",
        "lodash.clonedeep": "^4.5.0",
        "make-promises-safe": "^5.0.0",
        "premove": "^1.0.0",
        "qoa": "^0.2.0",
        "sade": "^1.6.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^3.0.2",
        "@slimio/psp": "^0.5.4",
        "ava": "^2.2.0",
        "cross-env": "^5.2.0",
        "eslint": "^6.1.0",
        "husky": "^3.0.2",
        "jsdoc": "^3.6.3",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js src/**.js commands/**.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
